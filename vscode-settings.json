	// "vim.statusBarColors.visualblock": "#FF0000",
	//  "vim.cursorStylePerMode.visualblock": "red",

	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": [
				"leader",
				"space"
			],
			"commands": [
				":nohl"
			]
		},
    {
      "before": ["u"],
      "after": [],
      "commands": [
        {
          "command": "undo"
        }
      ]
    },
    {
      "before": ["<C-r>"],
      "after": [],
      "commands": [
        {
          "command": "redo"
        }
      ]
    }
	],
