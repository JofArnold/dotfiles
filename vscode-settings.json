    "textMateRules": [
      {
        "name": "Children",
        "scope": "JSXNested",
        "settings": {
          "foreground": "#A1A9B7"
        }
      },
      {
        "name": "Comment Slashes",
        "scope": "source.js comment.line.double-slash.js",
        "settings": {
          "foreground": "#525865",
          "fontStyle": "italic"
        }
      },
      {
        "name": "js ts this",
        "scope": "punctuation.definition.comment.js",
        "settings": {
          "foreground": "#525865",
          "fontStyle": "italic"
        }
      },
      {
        "name": "js ts this",
        "scope": "var.this,variable.language.this.js,variable.language.this.ts,variable.language.this.jsx,variable.language.this.tsx",
        "settings": {
          "foreground": "#DC616A",
          "fontStyle": "italic"
        }
      },
      {
        "name": "End of line semis",
        "scope": "punctuation.terminator.statement.js",
        "settings": {
          "foreground": "#b7bdc8"
        }
      },
      {
        "name": "Curlies in jsx vars",
        "scope": "meta.brace.curly.js",
        "settings": {
          "foreground": "#b7bdc8"
        }
      },
      {
        "name": "Object keys",
        "scope": "constant.other.object.key.js string.unquoted.js",
        "settings": {
          "foreground": "#b7bdc8"
        }
      },
      {
        "name": "JSX tag",
        "scope": "meta.tag.jsx",
        "settings": {
          "foreground": "#dc616a"
        }
      },
      {
        "name": "JSX brackets",
        "scope": "punctuation.section.embedded.end.jsx, punctuation.section.embedded.begin.jsx",
        "settings": {
          "foreground": "#b7bdc8"
        }
      },
      {
        "name": "Source Json Meta Structure Dictionary Json > Value Json > String Quoted Json,source Json Meta Structure Array Json > Value Json > String Quoted Json,source Json Meta Structure Dictionary Json > Value Json > String Quoted Json > Punctuation,source Json Meta Structure Array Json > Value Json > String Quoted Json > Punctuation",
        "scope": "source.json meta.structure.dictionary.json > value.json > string.quoted.json,source.json meta.structure.array.json > value.json > string.quoted.json,source.json meta.structure.dictionary.json > value.json > string.quoted.json > punctuation,source.json meta.structure.array.json > value.json > string.quoted.json > punctuation",
        "settings": {
          "foreground": "#FF0000"
        }
