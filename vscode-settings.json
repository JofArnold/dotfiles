{
  // ----------------------------------------
  // Editor styles
  //
  "workbench.colorTheme": "One Dark Pro",
  "editor.fontSize": 13,
  "editor.cursorStyle": "line",
  "editor.renderIndentGuides": true,
  "editor.tabSize": 2,
  "editor.fontFamily": "Fira Code, Monaco, 'Courier New', monospace",
  "editor.lineHeight": 20,
  "editor.fontLigatures": true,
  "workbench.editor.showIcons": true,
  "editor.renderControlCharacters": true,
  "editor.minimap.enabled": false,
  "editor.selectionHighlight": false,
  "workbench.editor.showTabs": false,
  // ----------------------------------------
  // Terminal
  "terminal.integrated.fontFamily": "Fira Code",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.lineHeight": 1.5,
  "terminal.integrated.enableBell": false,
  // ----------------------------------------
  // Vim
  //
  "vim.enableNeovim": true,
  "vim.neovimPath": "nvim",
  "vim.easymotion": true,
  "vim.leader": "\\",
  "vim.hlsearch": true,
  "vim.visualstar": true,
  "vim.useCtrlKeys": true,
  //"vim.searchHighlightColor": "rgba(255,0,0,1)",
  "vim.searchHighlightColor": "#E1B870",

  // "vim.easymotionMarkerBackgroundColor": "black",
  // "vim.easymotionMarkerForegroundColorOneChar": "black",
  // "vim.easymotionMarkerForegroundColorTwoChar": "black",
  // "vim.easymotionMarkerFontFamily": "Source Code Pro",
  // "vim.easymotionMarkerHeight": 16,
  // "vim.easymotionMarkerYOffset": 12,
  // "vim.easymotionMarkerWidthPerChar": 9,

  //"vim.useSystemClipboard": true,
  // "vim.easymotionMarkerBackgroundColor": "#FF0000",
  // "vim.easymotionMarkerBackgroundColor": "#FF0000",
  // "vim.statusBarColors.visualblock": "#FF0000",
  //  "vim.cursorStylePerMode.visualblock": "red",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["leader", "space"],
      "commands": [":nohl"]
    }
  ],
  "window.title": "${folderPath}/${activeEditorShort} ${dirty}",
  "editor.autoClosingBrackets": false,
  "workbench.activityBar.visible": true,
  "workbench.statusBar.visible": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.panel.defaultLocation": "bottom",
  // ----------------------------------------
  // Prettier
  //
  "javascript.format.enable": false,
  "editor.formatOnSave": true,
  //"eslint.autoFixOnSave": false,
  "eslint.alwaysShowStatus": true,
  "prettier.eslintIntegration": true,

  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  //"search.location": "panel",
  "workbench.sideBar.location": "left",
  "debug.toolBarLocation": "floating",
  "window.zoomLevel": 0,
  "team.showWelcomeMessage": false,

